{
  "name": "story-test",
  "version": "1.0.0",
  "description": "A storyline adventure",
  "main": "./story.json",
  "scripts": {
    "compile": "storylines compile .",
    "chart": "storylines chart",
    "update-chart": "storylines compile . /tmp/story.json && storylines chart /tmp/story.json /tmp/data.json /tmp/data.dot && dot -Tsvg -o /tmp/chart.svg /tmp/data.dot",
    "view-chart": "storylines compile . /tmp/story.json && storylines chart /tmp/story.json /tmp/data.json /tmp/data.dot -v && dot -Tsvg -o /tmp/chart.svg /tmp/data.dot && xdg-open /tmp/chart.svg",
    "local": "storylines local ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Neamar/story-test.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Neamar/story-test/issues"
  },
  "homepage": "https://github.com/Neamar/story-test#readme",
  "dependencies": {},
  "devDependencies": {
    "nodemon": "^1.17.5",
    "storylines": "^2.1.7"
  }
}
